scrape_pfr_yearly <- function(year) {
Sys.sleep(2) # Be a good person
cat(glue::glue("Scraping {year}!"), "\n")
url <- glue::glue("https://www.pro-football-reference.com/years/{year}/fantasy.htm")
url %>%
read_html() %>%
html_table() %>%
.[[1]] %>%
set_names(nm = c(
"rank", "player", "team", "pos", "age", "g", "gs",
"pass_comp", "pass_att", "pass_yds", "pass_tds", "pass_int",
"rush_att", "rush_yds", "rush_ypa", "rush_tds", "rec_tar",
"rec", "rec_yds", "rec_ypr", "rec_tds", "fum", "fum_lost",
"total_tds", "two_pt", "two_pt_passes",
"fan_pts", "ppr", "dkpt", "fdpt", "vbd", "pos_rank", "overall_rank"
)) %>%
filter(g != "G") %>%
mutate(
player = str_remove_all(player, "\\*"),
player = str_remove_all(player, "\\+"),
year = year
) %>%
mutate(two_pt = case_when(two_pt == "" ~ two_pt_passes,
two_pt != "" ~ two_pt,
two_pt == NA_character_ ~ two_pt_passes,
TRUE ~ two_pt)) %>%
mutate_at(vars(rank, age:overall_rank), as.numeric) %>%
mutate(two_pt = replace_na(two_pt, 0))
}
library(tidyverse)
library(rvest)
df <- 2009:2018 %>%
map(scrape_pfr_yearly)
